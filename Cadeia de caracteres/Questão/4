#Leia uma frase e gere uma nova frase, duplicando cada caractere da frase digitada.

def duplicada():
    frase = str(input("Digite uma frase:"))
    nova_frase = ""
    nova_letra = ""
    for letra in frase:
             nova_letra = (letra + letra)
             print(letra)

             nova_frase = nova_frase + nova_letra
    print(nova_frase)
duplicada()
